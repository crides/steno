# Readonly dictionary. Dictionary editing is disabled
STENO_READONLY = no
# No display available for UI. Implies STENO_READONLY
STENO_NOUI = no
# No mass storage device enabled
STENO_NOMSD = no
# Graphical stroke display for demos
STENO_STROKE_DISPLAY = no
STENO_NOUNICODE = yes

STENO_DEBUG = hist # stroke flash dicted
STENO_LOG_BACKEND = flash # console
# Disables the internal orthography engine
STENO_NOORTHOGRAPHY = yes
